# API Configuration
REACT_APP_API_URL=http://localhost:3005
REACT_APP_API_VERSION=v1

# Feature Flags
REACT_APP_ENABLE_AI_FEATURES=true
REACT_APP_ENABLE_MARKETPLACE=true
REACT_APP_ENABLE_ANALYTICS=true

# Authentication (if needed)
REACT_APP_AUTH_ENABLED=false
REACT_APP_AUTH_DOMAIN=your-auth-domain
REACT_APP_AUTH_CLIENT_ID=your-client-id

# File Upload Configuration
REACT_APP_MAX_FILE_SIZE=10485760  # 10MB in bytes
REACT_APP_ALLOWED_FILE_TYPES=.pdf,.doc,.docx,.txt,.md

# Analytics (if using)
REACT_APP_ANALYTICS_ID=your-analytics-id

# Knowledge Base Configuration
REACT_APP_DEFAULT_KNOWLEDGE_BASE_ID=default
REACT_APP_MAX_KNOWLEDGE_BASES=5

# Development Mode
REACT_APP_DEV_MODE=true
REACT_APP_ENABLE_DEBUG_LOGGING=true

# Port Configuration
PORT=3005 